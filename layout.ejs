<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>
</head>
<body>  
    <table>
        <thead>
            <tr>
                <% header.forEach(value => {%>
                    <th><%= value%></th>
                <%})%>
            </tr>
        </thead>
        <tbody>
            <% for(var i = 0; i < rowsCount; i++){%>
                <tr>
                    <% body[i].forEach(value => {%>
                        <th><%= value%></th>
                    <%}) %>     
                </tr>
            <%} %>
        </tbody>
    </table>
</body>
</html>